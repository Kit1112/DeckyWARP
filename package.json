{
  "name": "DeckyWARP",
  "version": "1.5.0",
  "description": "Auto-installing of Cloudflare WARP and its management from the plugins menu. Using WARP on a steam deck has never been so easy!",
  "scripts": {
    "build": "shx rm -rf dist && rollup -c",
    "watch": "rollup -c -w",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kit1112/DeckyWARP.git"
  },
  "keywords": [
    "decky",
    "plugin",
    "vpn",
    "network",
    "utility",
    "root",
    "steam-deck",
    "deck"
  ],
  "author": "death_nick <boombeat.team@gmail.com>",
  "license": "free",
  "bugs": {
    "url": "https://github.com/Kit1112/DeckyWARP/issues"
  },
  "homepage": "https://github.com/Kit1112/DeckyWARP#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.5.0",
    "@types/react": "16.14.0",
    "@types/webpack": "^5.28.5",
    "rollup": "^2.79.2",
    "rollup-plugin-import-assets": "^1.1.1",
    "shx": "^0.3.4",
    "tslib": "^2.8.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "decky-frontend-lib": "^3.25.0",
    "react-icons": "^4.12.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "react",
        "react-dom"
      ]
    }
  }
}
